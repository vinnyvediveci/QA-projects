
public class PrintInWords {

	
	
	public static void printOneToTen()
	{
		String[]  myList = {"one", "two", "three", "four", "five", "six", "seven", "eight", "nine", "ten"}; 
        for (int i = 0; i < myList.length; i++) {
	        System.out.println(myList[i] + " ");
    }
     public static String printOneToHundred();
       {
    	   String[] myList2 = {"one", "two", "three", "four", "five", "six", "seven", "eight", "nine", "ten"}; 
    	   String[] myList3 = {"eleven", "twelve", "thirteen", "fourteen", "fifteen", "sixteen", "seventeen", "eighteen", "nineteen"};
    	   String[] myList4 = {"twenty", "thirty", "fourty", "fifty", "sixty", "seventy", "eighty", "ninety", "hundred"};
    	   
    	   System.out.println(myList2, myList3);
    	   
    	   for (int i = 0; i < myList4.length; i++) {
    		   for (int j = 0; j < myList2.length; j++) {	
				System.out.println(myList5[i][j]);
    		   }
    	   }
       }
        
  }
}