
public class Maths {

	public static void main(String[] args) {
		System.out.println(addTwo(13, 14));
		System.out.println(multiplyTwo(2, 4));
		System.out.println(subtractTwo(116, 3));
		System.out.println(divideTwo(50, 4));
	}
	
	public static int addTwo(int a, int b) {
		return a + b;
	}

	public static int multiplyTwo(int a, int b) {
		return a * b;
	}
	
	public static int subtractTwo(int a, int b) {
		return a - b;
	}
	
	public static double divideTwo(int a, int b) {
		return a / b;
	}
	
	
}
